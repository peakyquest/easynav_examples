                <always_on>1</always_on>
                <update_rate>20</update_rate>
                <camera name="camera_front">
                    <horizontal_fov>1.0472</horizontal_fov>
                    <lens>
                        <intrinsics>
                          <!-- fx = fy = width / ( 2 * tan (hfov / 2 ) ) -->
                          <fx>554.3</fx>
                          <fy>554.3</fy>
                          <!-- cx = ( width + 1 ) / 2 -->
                          <cx>320.5</cx>
                          <!-- cy = ( height + 1 ) / 2 -->
                          <cy>240.5</cy>
                          <s>0</s>
                        </intrinsics>
                    </lens>
                    <distortion>
                        <k1>0.0</k1>
                        <k2>0.0</k2>
                        <k3>0.0</k3>
                        <p1>0.0</p1>
                        <p2>0.0</p2>
                        <center>0.5 0.5</center>
                    </distortion>
                    <image>
                        <width>640</width>
                        <height>480</height>
                        <format>R8G8B8</format>
                    </image>
                    <clip>
                        <near>0.01</near>
                        <far>300</far>
                    </clip>
                    <depth_camera>
                      <clip>
                        <near>0.1</near>
                        <far>10</far>
                      </clip>
                    </depth_camera>
                    <noise>
                        <type>gaussian</type>
                        <mean>0</mean>
                        <stddev>0.007</stddev>
                    </noise>
                </camera>
            </sensor>

            <sensor name='navsat' type='navsat'>
